package = swift.phoenix_crypto
language = Objective-C

headers = PhoenixCrypto/PhoenixCrypto-Swift.h
headerFilter = PhoenixCrypto/*

staticLibraries = libPhoenixCrypto.a

linkerOpts = -L/usr/lib/swift
linkerOpts.ios_x64 = -platform_version ios-simulator 16.0.0 16.0.0 -L/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/swift/iphonesimulator/
linkerOpts.ios_arm64 = -iphoneos_version_min 16.0.0 -L/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/swift/iphoneos/
linkerOpts.ios_simulator_arm64 = -platform_version ios-simulator 16.0.0 16.0.0 -L/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/swift/iphonesimulator/
